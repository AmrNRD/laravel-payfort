{
  "name": "moeen-basra/laravel-payfort",
  "description": "Laravel package for Payfort payment gateway",
  "type": "library",
  "require": {
    "php": "^7.2",
    "moneyphp/money": "^3.2",
    "guzzlehttp/guzzle": "^6.3",
    "ext-json": "*",
    "ext-curl": "*"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Moeen Basra",
      "email": "m.basra@live.com"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "^8.3",
    "orchestra/testbench": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "MoeenBasra\\Payfort\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "MoeenBasra\\Payfort\\PayfortServiceProvider"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
